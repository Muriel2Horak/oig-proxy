[run]
branch = True
parallel = True
source =
    addon/oig-proxy
omit =
    */tests/*
    */testing/*
    */analysis/*
    */.venv/*
    */__pycache__/*
    */logs/*
    */reports/*
    */addon/oig-proxy/proxy.py
    */addon/oig-proxy/mqtt_publisher.py
    */addon/oig-proxy/cloud_manager.py
    */addon/oig-proxy/cloud_session.py
    */addon/oig-proxy/utils.py
    */addon/oig-proxy/main.py

[report]
show_missing = True
skip_covered = True
precision = 1
